{
  "expo": {
    "name": "ComuniApp",
    "slug": "apptcc",
    "scheme": "apptcc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#451458"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon2.png",
        "backgroundColor": "#451458"
      },
      "package": "com.michelmarques.apptcc"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}